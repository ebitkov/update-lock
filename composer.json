{
    "name": "ebitkov/update-lock-bundle",
    "description": "Locks access to a Symfony app while it's updating to prevent errors",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Emanuel Bitkov",
            "email": "emanuel.bitkov@regenta-verlag.de"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ebitkov/app-lock-composer-plugin": "1.0.1"
    },
    "autoload": {
        "psr-4": {
            "Ebitkov\\UpdateLockBundle\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "ebitkov/app-lock-composer-plugin": true
        }
    }
}
